<?xml version="1.0" encoding="utf-8"?>
<!--
   Minimal data extraction rules - exclude all data since meaningful data is stored in Supabase
   All user data syncs automatically when user logs in on new device
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude all SharedPreferences (auth tokens, app state, user prefs) -->
        <exclude domain="sharedpref" path="."/>
        
        <!-- Exclude all files (cache, logs, temporary data) -->
        <exclude domain="file" path="."/>
        
        <!-- Exclude all databases (if any local databases exist) -->
        <exclude domain="database" path="."/>
    </cloud-backup>
    
    <device-transfer>
        <!-- Exclude all SharedPreferences (auth tokens, app state, user prefs) -->
        <exclude domain="sharedpref" path="."/>
        
        <!-- Exclude all files (cache, logs, temporary data) -->
        <exclude domain="file" path="."/>
        
        <!-- Exclude all databases (if any local databases exist) -->
        <exclude domain="database" path="."/>
    </device-transfer>
</data-extraction-rules>